{
    "name": "ernestobaezf/ltoolkit",
    "description": "Package to easy start a new project by using repository and other patterns for laravel",
    "keywords": ["repository", "laravel", "package", "manager", "structure", "standard", "repository patterns", "easy start", "boilerplate"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ernesto Baez",
            "homepage": "https://github.com/ernestobaezf"
        }
    ],
    "autoload": {
        "psr-4": {
            "LToolkit\\": "src/"
        },
        "files": [
            "src/globals.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "LToolkit\\Test": "src/tests"
        }
    },
    "require": {
        "php": "~7.1",
        "psr/http-client": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "prettus/l5-repository": "^2.6"
    },
    "require-dev": {
        "nunomaduro/larastan": "^0.3.16",
        "dg/bypass-finals": "^1.1",
        "phpunit/phpunit": "~8.1"
    }
}
